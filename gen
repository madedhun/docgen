Private Sub datum_Exit(Cancel As Integer)
Dim vmi As String

datum.SetFocus
intdatum = datum.Text

vmi = DateAdd("d", 45, intdatum)
vmi = Format(CDate(vmi), "Long Date")
'MsgBox (Len(vmi))

scontromez.SetFocus
scontromez.Text = vmi
datum.SetFocus
intdatum = Format(CDate(datum.Text), "Long Date")
scontromez.SetFocus

'MsgBox (intdatum)

End Sub

Sub hivfelj_Click()
Dim fso As Object
Set fso = CreateObject("Scripting.FileSystemObject")

If ReportFileStatus("d:\Users\PappD\Documents\ügyek\2015\" & mez(1) & "\" & mez(1) & "-2015_hivatalos_feljegyzes.html") = True Then 'achtung!!  nem jó a helye



' A Msgbox és a feltétel az ismétlés ellen
MsgBox ("A " & mez(1) & "/2015." & Chr$(13) & mez(3) & " ügyében ilyet csináltál!!!")
Else

Dim file As Object
Set file = fso.CreateTextFile("d:\Users\PappD\Documents\ügyek\2015\" & mez(1) & "\" & mez(1) & "-2015_hivatalos_feljegyzes.html") 'achtung!!  nem jó a helye
file.WriteLine "<!DOCTYPE HTML PUBLIC ""-//W3C//DTD HTML 4.0 Transitional//EN"" > "
file.WriteLine "<HTML>"
file.WriteLine "<HEAD>"
file.WriteLine "    <META HTTP-EQUIV=""CONTENT-TYPE"" CONTENT=""text/html; charset=windows-1250"">"
file.WriteLine "    <TITLE></TITLE>"
file.WriteLine "    <META NAME=""GENERATOR"" CONTENT=""LibreOffice 3.6  (Windows)"">"
file.WriteLine "    <META NAME=""CHANGED"" CONTENT=""20150326;13250776"">"
file.WriteLine "     <META NAME=""Információ 1"" CONTENT="""">"
file.WriteLine "     <META NAME=""Információ 2"" CONTENT="""">"
file.WriteLine "     <META NAME=""Információ 3"" CONTENT="""">"
file.WriteLine "     <META NAME=""Információ 4"" CONTENT="""">"
file.WriteLine "     <STYLE TYPE=""text/css"">"
file.WriteLine "     <!--"
file.WriteLine "         @page { margin-left: 2cm; margin-right: 2cm; margin-top: 2.5cm; margin-bottom: 2cm }"
file.WriteLine "         @page:first { margin-top: 2cm; margin-bottom: 2.5cm }"
file.WriteLine "         P { margin-right: 0.5cm; margin-bottom: 0.21cm; direction: ltr; color: #000000; widows: 0; orphans: 0 }"
