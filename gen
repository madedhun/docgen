file.WriteLine "    <TITLE></TITLE>"
file.WriteLine "    <META NAME=""GENERATOR"" CONTENT=""LibreOffice 3.6  (Windows)"">"
file.WriteLine "    <META NAME=""CHANGED"" CONTENT=""20150326;13250776"">"
file.WriteLine "     <META NAME=""Információ 1"" CONTENT="""">"
file.WriteLine "     <META NAME=""Információ 2"" CONTENT="""">"
file.WriteLine "     <META NAME=""Információ 3"" CONTENT="""">"
file.WriteLine "     <META NAME=""Információ 4"" CONTENT="""">"
file.WriteLine "     <STYLE TYPE=""text/css"">"
file.WriteLine "     <!--"
file.WriteLine "         @page { margin-left: 2cm; margin-right: 2cm; margin-top: 2.5cm; margin-bottom: 2cm }"
file.WriteLine "         @page:first { margin-top: 2cm; margin-bottom: 2.5cm }"
file.WriteLine "         P { margin-right: 0.5cm; margin-bottom: 0.21cm; direction: ltr; color: #000000; widows: 0; orphans: 0 }"
file.WriteLine "         P.western { font-family: ""Times New Roman"", serif; font-size: 12pt; so-language: hu-HU }"
file.WriteLine "         P.cjk { font-family: ""SimSun""; font-size: 12pt; so-language: zh-CN }"
file.WriteLine "         P.ctl { font-family: ""Mangal""; font-size: 12pt; so-language: hi-IN }"
