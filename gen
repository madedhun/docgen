Sub adatkerolevelszekhely_Click()

Dim fso As Object
Set fso = CreateObject("Scripting.FileSystemObject")

If ReportFileStatus("d:\Users\PappD\Documents\ügyek\2015\" & mez(1) & "\" & mez(1) & "-2015_adatkero_szekhelyes.html") = True Then 'achtung!!  nem jó a helye



' A Msgbox és a feltétel az ismétlés ellen
MsgBox ("A " & mez(1) & "/2015." & Chr$(13) & mez(3) & " ügyében ilyet csináltál!!!")
Else

Dim file As Object
Set file = fso.CreateTextFile("d:\Users\PappD\Documents\ügyek\2015\" & mez(1) & "\" & mez(1) & "-2015_adatkero_szekhelyes.html") 'achtung!!  nem jó a helye
file.WriteLine "<!DOCTYPE HTML PUBLIC ""-//W3C//DTD HTML 4.0 Transitional//EN"" > "
file.WriteLine "<HTML>"
file.WriteLine "<HEAD>"
file.WriteLine "    <META HTTP-EQUIV=""CONTENT-TYPE"" CONTENT=""text/html; charset=windows-1250"">"
file.WriteLine "    <TITLE></TITLE>"
