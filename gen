Function ReportFileStatus(filespec) As Boolean 'létezik-e a fájl funció
   Dim letezik As Boolean
   
   Dim fso As Object
   Set fso = CreateObject("Scripting.FileSystemObject")
   If (fso.FileExists(filespec)) Then
     ReportFileStatus = True
     
   Else
     ReportFileStatus = False
     
   End If

End Function

