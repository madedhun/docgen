file.WriteLine "</B></FONT><FONT SIZE=3 STYLE=""font-size: 11pt""><B>2015. 05. 10.</B></FONT></P>" ' & mez(16) &
file.WriteLine "<P CLASS=""western"" ALIGN=JUSTIFY STYLE=""margin-bottom: 0cm""><FONT SIZE=3 STYLE=""font-size: 11pt""> </FONT></P>"
file.WriteLine "<DIV TYPE=FOOTER>"
file.WriteLine "    <P STYLE=""margin-bottom: 0cm""><BR>"
file.WriteLine "    </P>"
file.WriteLine "</DIV>"
file.WriteLine "</BODY>"
file.WriteLine "</HTML>"





file.WriteLine ""
file.WriteLine ""





file.Close


Set fso = Nothing
Set file = Nothing

End If

End Sub

Sub mukodj_Click()

Dim rs As DAO.Recordset
Dim strSQL As String
Dim szam As Double
Dim megvan As Boolean
ReDim mez(28) As String
Dim betolt As String
Dim fasz As String
Dim fso As Object
Set fso = CreateObject("Scripting.FileSystemObject")
Dim nincs As Object
Dim kep As Object




szambe.SetFocus
If szambe.Text = "" Then
MsgBox ("Nem írtál be számot!!! most jól hibaüznetet fog dobni Kattintsál az End-re !")
End If

szam = szambe.Text
'MsgBox (szam)

strSQL = "SELECT * FROM Civil_adatbazis"  'WHERE Ügyszám (saját)"" =""" & szam & """

Set rs = CurrentDb.OpenRecordset(strSQL)


rs.MoveFirst

While Not rs.EOF

If rs.Fields(1).Value = szam Then
megvan = True

i = 0
While i < 29


If rs.Fields(i) = "" Then
fasz = Str(rs.Fields(i).Value)
mez(i) = fasz

MsgBox ("kaki")
End If



If rs.Fields(i).Value <> "" Then

mez(i) = rs.Fields(i).Value
'MsgBox (rs.Fields(i).Value)
'MsgBox ("másik kaki")
mez(i) = rs.Fields(i).Value


End If


'MsgBox (mez(i))
i = i + 1
Wend

   
    szervezetneve.SetFocus
    szervezetneve.Text = rs.Fields(3).Value
    
    intezkedes.SetFocus
    intezkedes.Text = rs.Fields(17).Value
    
    
    teendok.SetFocus
    teendok.Text = rs.Fields(23).Value
    
    
    
    If rs.Fields(3).Value Like "*közh*" Then 'kétpontosok
        
        If rs.Fields(4).Value Like "T*" Then  'kétponos egyesület
      
            tipus.SetFocus
            tipus.Text = "nevében közhasznú egyesület"
        Else 'kétpontos alapítvány
            tipus.SetFocus
            tipus.Text = "nevében közhasznú alapítvány"
        End If
        
        
    Else 'egypontosok
        If rs.Fields(4).Value Like "T*" Then
            tipus.SetFocus
            tipus.Text = "egyesület"
        Else
        
            tipus.SetFocus
            tipus.Text = "alapítvány"
        End If
    
    End If
    
    
     ezmegvan.SetFocus
         
        
        If rs.Fields(17).Value = rs.Fields(23).Value Then
            
               
        ezmegvan.Text = "EZ már kiment, mást kéne csinálni"
        Else
        ezmegvan.Text = ""
            
        End If
        

End If



rs.MoveNext
If megvan = False Then

If rs.EOF = True Then
MsgBox ("nincs ilyen szám")
End If

End If

Wend

If megvan = True Then
If mez(7) <> "" Then
mez(7) = Format(CDate(mez(7)), "Long Date")
End If

                If mez(8) <> "" Then
               
                mez(8) = Format(CDate(mez(8)), "Long Date")
               ' Else
                'hibás a kóóóód
               ' MsgBox ("Ebben még nem jogerős a törlő végzés !!!!!" & Chr$(13) & Chr$(13) & "Ezért nem csinálok semmit")
                End If
                
                
                
If mez(16) <> Null Then
mez(16) = Format(CDate(mez(16)), "Long Date")
End If
If mez(18) <> Null Then
mez(18) = Format(CDate(mez(18)), "Long Date")
End If
If mez(19) <> Null Then
mez(19) = Format(CDate(mez(19)), "Long Date")
End If
If mez(22) <> Null Then
mez(22) = Format(CDate(mez(22)), "Long Date")
End If
'MsgBox (mez(28))

If fso.FolderExists("d:\Users\PappD\Documents\ügyek\2015\" & mez(1)) = False Then
Set nincs = fso.CreateFolder("d:\Users\PappD\Documents\ügyek\2015\" & mez(1))

End If

fso.CopyFile "D:\fejlec.png", "D:\Users\PappD\Documents\ügyek\2015\" & mez(1) & "\", True    ', true)
End If

If mez(26) = True Then
Me![kep].Picture = "d:\Users\PappD\Documents\ügyek\körleveles doksik eredeteije\van.png"
'MsgBox ("van")

Else
Me![kep].Picture = "d:\Users\PappD\Documents\ügyek\körleveles doksik eredeteije\nincs.png"
'MsgBox ("nincs")

End If

datum.SetFocus
intdatum = datum.Text




vmi = DateAdd("d", 45, intdatum)
vmi = Format(CDate(vmi), "Long Date")
'MsgBox (Len(vmi))

'scontromez.SetFocus

'scontromez.Text = vmi
datum.SetFocus
intdatum = Format(CDate(datum.Text), "Long Date")

'MsgBox (intdatum)

End Sub

