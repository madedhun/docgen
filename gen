Sub mintajelz_Click()

Dim fso As Object
Set fso = CreateObject("Scripting.FileSystemObject")

If ReportFileStatus("d:\Users\PappD\Documents\ügyek\2015\" & mez(1) & "\" & mez(1) & "-2015_jelzes.html") = True Then
MsgBox ("A " & mez(1) & "/2015." & Chr$(13) & mez(3) & " ügyében ilyet csináltál!!!")
Else

Dim file As Object
Set file = fso.CreateTextFile("d:\Users\PappD\Documents\ügyek\2015\" & mez(1) & "\" & mez(1) & "-2015_jelzes.html")

mez(7) = Format(CDate(mez(7)), "Long Date")
'fejléc
file.WriteLine "<!DOCTYPE HTML PUBLIC ""-//W3C//DTD HTML 4.0 Transitional//EN"" > "
file.WriteLine "<HTML>"
